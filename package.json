{
  "name": "i0c-redirect-worker",
  "version": "0.1.0",
  "private": true,
  "pnpm": {
    "overrides": {
      "@vercel/fun>tar": "7.5.7",
      "@vercel/fun>path-to-regexp": "8.3.0",
      "@vercel/node>path-to-regexp": "6.3.0",
      "@vercel/remix-builder>path-to-regexp": "6.3.0",
      "@vercel/node>undici": "7.19.2",
      "@vercel/blob>undici": "7.19.2",
      "miniflare>undici": "7.19.2",
      "tar": "7.5.7",
      "qs": "6.14.1"
    }
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts && node scripts/post-build.mjs",
    "dev:cf": "wrangler dev",
    "deploy:cf": "wrangler deploy",
    "build:vc": "pnpm build",
    "build:vc:watch": "tsup --config tsup.config.ts --watch --onSuccess \"node scripts/post-build.mjs\"",
    "dev:vc": "concurrently \"pnpm build:vc:watch\" \"vercel dev\"",
    "deploy:vc": "vercel --prod",
    "build:nf": "pnpm build",
    "dev:nf": "npx --yes netlify-cli@latest dev",
    "deploy:nf": "npx --yes netlify-cli@latest deploy --build"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251213.0",
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "concurrently": "^9.2.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vercel": "^50.9.5",
    "wrangler": "^4.61.1"
  }
}
